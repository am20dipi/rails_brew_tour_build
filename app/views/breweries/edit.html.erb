<h1>Edit <%= @brewery.name %></h1>
<%= render partial: "beers/errors", locals: {object: @brewery} %>
<br>
<div>
    <%= form_with(model: @brewery) do |f| %>
        <%= render partial: "form", locals: {form_builder: f} %> 
        <div class="form-button">
            <%= f.submit %>
        </div>
    <% end %>
</div>

<br>
<%= link_to "Go Back", :back %>