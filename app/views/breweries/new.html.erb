<h1>Add a New Brewery</h1>
<%= render partial: "beers/errors", locals: {object: @brewery} %>
<br>
<div class="content">
    <%= form_with(model: @brewery) do |f| %>
        <%= render partial: "form", locals: {form_builder: f} %> 
        <div class="form-button">
            <%= f.submit %>
        </div>

        <div class="bottom-nav">
            <br><a class="bottom-nav-link <%= active_css(breweries_path) %>" href="/breweries">Nevermind!</a>
        </div>
    <% end %>
</div>

