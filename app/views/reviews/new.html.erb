<% if @beer %>
    <h1>New Review for <%= @beer.name %></h1>
<% else %>
    <h1>New Review</h1>
<% end %>

<%= render partial: "beers/errors", locals: {object: @review}%>


<%= form_for ([@beer, @review]) do |f| %>
    <%= render partial: "form", locals: {form_builder: f} %>
    <div class="form-button">
            <%= f.submit %>
    </div>
<% end %>