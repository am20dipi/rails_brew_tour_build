<h1>Edit Your <%= @review.beer.name %> Review</h1>

<br>
<div>
    <%= form_with(model: @review) do |f| %>
        <%= render partial: "form", locals: {form_builder: f} %> 
        
        <div class="form-button">
            <%= f.submit %>
        </div>
    <% end %>
</div>

<div class="bottom-nav">
    <a class="bottom-nav-link <%= active_css(review_path(@review)) %>" href="/reviews/<%= @review.id %>">Nevermind!</a>
</div>